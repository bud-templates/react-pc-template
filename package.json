{
  "name": "bud-web3tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development REACT_APP_ENV=development webpack-dev-server -c build/webpack.dev.js",
    "start": "cross-env NODE_ENV=development REACT_APP_ENV=development  webpack-dev-server -c build/webpack.dev.js",
    "start:master": "cross-env NODE_ENV=development REACT_APP_ENV=master  webpack-dev-server -c build/webpack.dev.js",
    "start:alpha": "cross-env NODE_ENV=development REACT_APP_ENV=alpha  webpack-dev-server -c build/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=development REACT_APP_ENV=prod  webpack-dev-server -c build/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=production REACT_APP_ENV=master webpack -c build/webpack.prod.js",
    "build:master": "cross-env NODE_ENV=production REACT_APP_ENV=master webpack -c build/webpack.prod.js",
    "build:alpha": "cross-env NODE_ENV=production REACT_APP_ENV=alpha webpack -c build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production REACT_APP_ENV=prod webpack -c build/webpack.prod.js",
    "build:analy": "cross-env NODE_ENV=production REACT_APP_ENV=prod webpack -c build/webpack.analy.js",
    "eslint": "eslint --max-warnings=0 --fix",
    "pre-check": "tsc && npx lint-staged",
    "stylelint": "stylelint \"src/**/*.{css,less,scss}\" --fix"
  },
  "proxy": "http://localhost:3005",
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn eslint"
    ],
    "src/**/*.{css,less,scss}": [
      "stylelint \"src/**/*.{css,less,scss}\" --fix"
    ]
  },
  "dependencies": {
    "antd": "^5.0.2",
    "axios": "^1.2.0",
    "core-js": "^3.26.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.4.3",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-config": "^5.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^9.1.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.28.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "glob-all": "^3.3.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "lint-staged": "12.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-dev-utils": "^12.0.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
